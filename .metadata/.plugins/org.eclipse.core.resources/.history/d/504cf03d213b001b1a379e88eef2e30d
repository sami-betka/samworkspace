package cote.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Entity;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
public class Bet implements Serializable{
	
	private Date date;
	private int race;
	private int reunion;
	private boolean won;

}
